{
  "name": "profe-ai-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo for ProfeAI - React Frontend and Node.js Backend",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "frontend:dev": "npm run dev -w frontend",
    "frontend:build": "npm run build -w frontend",
    "backend:dev": "npx tsx backend/src/server.ts",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "test": "npm run test:coverage:check",
    "start": "npx tsx backend/src/server.ts",
    "healthcheck": "npm run healthcheck -w frontend",
    "test:coverage:check": "npm run test:coverage:check -w frontend",
    "build": "npm run frontend:build",
    "prepare": "husky"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "husky": "^9.1.7"
  },
  "overrides": {
    "qs": "6.14.2"
  },
  "dependencies": {
    "qs": "6.14.2"
  }
}
