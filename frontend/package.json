{
  "name": "profe-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm start\" \"npm run dev\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "tsx server.ts",
    "healthcheck": "npm run lint && npm test",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/tests/flashcard/*.test.{js,jsx}",
    "test:integration": "vitest run src/tests/flashcard/integration/*.integration.test.jsx",
    "test:a11y": "pa11y-ci",
    "ci:a11y": "start-server-and-test dev http://127.0.0.1:5173 test:a11y",
    "test:lighthouse": "lhci autorun",
    "ci:lighthouse": "start-server-and-test dev http://127.0.0.1:5173 test:lighthouse",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:security": "snyk test",
    "test:coverage:check": "vitest run --coverage",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "doc": "typedoc"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.974.0",
    "@genkit-ai/ai": "^1.27.0",
    "@genkit-ai/core": "^1.27.0",
    "@genkit-ai/googleai": "^1.27.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/openai": "^1.1.3",
    "@tavily/core": "^0.6.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "genkit": "^1.27.0",
    "genkitx-openai": "^0.29.0",
    "helmet": "^8.1.0",
    "langchain": "^1.2.8",
    "node-fetch": "^3.3.2",
    "react-router-dom": "^7.12.0",
    "tsx": "^4.21.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@eslint/js": "^9.36.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@sentry/react": "^10.27.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "baseline-browser-mapping": "^2.9.15",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^10.1.11",
    "firebase": "^12.6.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "pa11y-ci": "^4.0.1",
    "playwright": "^1.57.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.5",
    "snyk": "^1.1301.2",
    "start-server-and-test": "^2.1.3",
    "storybook": "^10.1.11",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.2.4",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vitest": "^4.0.13"
  }
